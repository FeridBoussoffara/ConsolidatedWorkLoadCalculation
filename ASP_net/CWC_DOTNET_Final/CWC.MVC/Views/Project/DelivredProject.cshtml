@model IEnumerable<CWC.MVC.Models.ProjectViewModel>

@{
    ViewBag.Title = "DelivredProject";
}

<h2>DelivredProject</h2>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.StartDate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.EndDate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Budget)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Category)
        </th>
       
        
        <th></th>
    </tr>

@foreach (var item in @ViewBag.delproj)
{
    <tr>

        <td>
            @item.Name
        </td>
        <td>
            @item.StartDate
        </td>
        <td>
            @item.EndDate
        </td>
        <td>
            @item.Budget
        </td>

        <td>
            @item.Category
        </td>
        
    </tr>
}

</table>
@Html.ActionLink("Back to Project's List", "Index", "", new { @class = "btn btn-primary" })


